<h1 style="width:100%; text-align:left;">Listing Item Histories</h1>
<span class="white orange button"><%= link_to 'New Item history', new_item_history_path %></span>
<br><br>
<%= will_paginate %>
<br>
<table>
  <tr>
    <th>User</th>
    <th>Item</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Signature</th>
    <th>Stamp</th>
    <th>Runningdistance</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @item_histories.each do |item_history| %>
  <tr>
    <td><%= item_history.user_id %></td>
    <td><%= item_history.item_id %></td>
    <td><%= item_history.latitude %></td>
    <td><%= item_history.longitude %></td>
    <td><%= item_history.signature %></td>
    <td><%= item_history.stamp %></td>
    <td><%= item_history.runningdistance %></td>
    <td><span class="button white orange"><%= link_to 'Show', item_history %></span></td>
    <td><span class="button white black"><%= link_to 'Edit', edit_item_history_path(item_history) %></span></td>
    <td><span class="button white red"><%= link_to 'Destroy', item_history, confirm: 'Are you sure?', method: :delete %></span></td>
  </tr>
<% end %>
</table>

<%= will_paginate %>

<br />
