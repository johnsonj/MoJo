<h1>Showing the backpack for <%= current_user.username %></h1>


<table>
  <tr>
    <th>User</th>
    <th>Name</th>
    <th>Description</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Image</th>
    <th>Rarity</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @items.each do |item| %>
  <tr>
    <td><%= item.user_id %></td>
    <td><%= item.name %></td>
    <td><%= item.description %></td>
    <td><%= item.latitude %></td>
    <td><%= item.longitude %></td>
    <td><%= item.image_id %></td>
    <td><%= item.rarity %></td>
    <td><%= link_to 'Show', item %></td>
    <td><%= link_to 'Edit', edit_item_path(item) %></td>
    <td><%= link_to 'Destroy', item, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Item', new_item_path %>
