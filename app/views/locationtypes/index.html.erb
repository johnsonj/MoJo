<h1>Listing locationtypes</h1>

<table>
  <tr>
    <th>Type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @locationtypes.each do |locationtype| %>
  <tr>
    <td><%= locationtype.type %></td>
    <td><%= link_to 'Show', locationtype %></td>
    <td><%= link_to 'Edit', edit_locationtype_path(locationtype) %></td>
    <td><%= link_to 'Destroy', locationtype, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

