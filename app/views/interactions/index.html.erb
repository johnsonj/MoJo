<h1>Listing interactions</h1>

<table>
  <tr>
    <th>Loclatitude</th>
    <th>Loclongitude</th>
    <th>Locationtype</th>
    <th>Persona</th>
    <th>Personb</th>
    <th>Timestamp</th>
    <th>Deltime</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @interactions.each do |interaction| %>
  <tr>
    <td><%= interaction.Loclatitude %></td>
    <td><%= interaction.loclongitude %></td>
    <td><%= interaction.LocationType %></td>
    <td><%= interaction.PersonA %></td>
    <td><%= interaction.PersonB %></td>
    <td><%= interaction.Timestamp %></td>
    <td><%= interaction.deltime %></td>
    <td><%= link_to 'Show', interaction %></td>
    <td><%= link_to 'Edit', edit_interaction_path(interaction) %></td>
    <td><%= link_to 'Destroy', interaction, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

